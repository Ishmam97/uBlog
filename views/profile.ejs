<body>
<!-- NAVbAR -->
<div class="cover">

</div>
<div class="nav">
  <div class="home-btn"><a href="#"><i class="fas fa-home "></i></a></div>
  <div class="searchbar">
  <input type="text" class="search-text" placeholder="Search">
  <a href="#" class="search-btn">
  <i class="fas fa-search"></i>
  </a>
  </div>

</div>
<div class="bp">

</div>


<div class="page-wrapper">
   <div class="page content-1">
   <!-- all content goes here -->
    <div class="shadow1" id="profile">
      <div id="dp">
        <div class="buttons-1">
              <a href="#"><button class="btn btn-1">Subscribe</button></a>
              <a href="#"><button class="btn btn-2">+</button></a>
        </div>
      </div>
      <div id="aboutme">
        <div class="aboutme">
          <h2 >About me</h2>
           <p>Lorem ipsum dolor sit amet. Voluptatibus, sequi consequuntur nisi, cum dicta natus deserunt aspernatur rerum eveniet..</p>
           <a href="#">Read more!</a>
        </div>
       </div>
    </div>


    <div class="shadow1" id="info">

      <div class="stats shadow1">
        <h1 id="numPosts"><%=numposts%></h1>
        <footer>posts</footer>
      </div>
      <div class="stats shadow1">
        <h1><%=followers%></h1>
        <footer>followers</footer>
      </div>
      <div class="stats shadow1">
        <h1><%=friends%></h1>
        <footer>Friends</footer>
      </div>

    </div>


    <div id="name-box">

      <h3><%=name%></h3>
      <footer><em>Food, Travel, Music</em></footer>

    </div>
    <div class="shadow1" id="top-post">

    </div>


   </div>
   <!-- load other posts -->
<div class="page content-2">
  <div class="box"></div>
  <div class="box"></div>
  <div class="box"></div>
  <div class="box"></div>
  <div class="box"></div>
   <div class="box"></div>
    <div class="box"></div>
  <div class="box"></div>
</div>

</div>
 <!-- footer -->
 <div class="main-footer">

 </div>

